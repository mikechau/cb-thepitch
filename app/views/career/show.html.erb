<h1><%= @pitch.did %></h1>
<h1><%= @pitch.company %></h1>
<h1><%= @pitch.job_title %></h1>


<p><%= "Applicant: #{@pitch.first_name} #{@pitch.last_name}" %></p>
<p><%= "Video link: #{@pitch.video_link}" %></p>
<p><%= "email: #{@pitch.email}" %></p>


<p><%= notice %></p>

<p><%= link_to "Delete", pitch_url(@pitch.id), :method => :delete %></p>
<p><%= link_to "Edit", edit_pitch_url(@pitch.id) %></p>
<p><%= link_to "Back to index", pitches_url %></p>


